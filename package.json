{
  "name": "coderunner",
  "version": "1.0.0",
  "description": "CodeRunner - A comprehensive code execution and project management platform",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:coverage": "NODE_ENV=test jest --coverage",
    "test:ci": "NODE_ENV=test jest --ci --coverage --watchAll=false",
    "test:unit": "NODE_ENV=test jest --testPathPatterns=tests/unit",
    "test:integration": "NODE_ENV=test jest --testPathPatterns=tests/integration",
    "test:websocket": "NODE_ENV=test jest tests/websocket",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:security": "npm audit && npm run test:security:owasp",
    "test:security:owasp": "echo 'OWASP security tests would run here'",
    "test:performance": "artillery run tests/performance/api-load-test.yml",
    "test:contract:provider": "jest --testPathPattern=tests/contracts/provider",
    "test:contract:consumer": "jest --testPathPattern=tests/contracts/consumer",
    "test:smoke": "jest --testPathPattern=tests/smoke",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:websocket && npm run test:e2e",
    "test:validation:functional": "node test-functional-validation.js",
    "test:validation:performance": "node test-performance-validation.js",
    "test:validation:security": "node test-security-validation.js",
    "test:validation:e2e": "node test-e2e-validation.js",
    "test:validation:integration": "node test-integration-validation.js",
    "test:validation:all": "node run-all-validations.js",
    "migrate": "ts-node src/migrations/run-migrations.ts run",
    "migrate:status": "ts-node src/migrations/run-migrations.ts status",
    "migrate:create": "ts-node src/migrations/run-migrations.ts create",
    "db:init": "npm run migrate",
    "db:reset": "echo \"Warning: This will drop all tables. Use with caution. Not implemented yet.\"",
    "db:health": "ts-node -e \"import {DatabaseService} from './src/services/database'; const db = DatabaseService.getInstance(); db.connect().then(() => db.healthCheck()).then(r => console.log(JSON.stringify(r, null, 2))).finally(() => db.disconnect());\""
  },
  "keywords": [
    "coderunner",
    "code-execution",
    "project-management",
    "typescript",
    "express",
    "postgresql"
  ],
  "author": "CodeRunner Team",
  "license": "MIT",
  "dependencies": {
    "agentsphere-js": "^1.0.3",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^3.0.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^8.0.1",
    "express-session": "^1.18.2",
    "express-validator": "^7.2.1",
    "helmet": "^7.1.0",
    "hpp": "^0.2.3",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "passport": "^0.7.0",
    "passport-github2": "^0.1.12",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.11.3",
    "prom-client": "^15.1.0",
    "socket.io": "^4.7.5",
    "uuid": "^9.0.0",
    "validator": "^13.15.15",
    "xss": "^1.0.15"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.9.0",
    "@pact-foundation/pact": "^12.1.0",
    "@playwright/test": "^1.40.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.18",
    "@types/express-rate-limit": "^5.1.3",
    "@types/express-session": "^1.18.2",
    "@types/hpp": "^0.2.6",
    "@types/jest": "^30.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/morgan": "^1.9.5",
    "@types/node": "^20.8.0",
    "@types/node-cron": "^3.0.11",
    "@types/passport": "^1.0.17",
    "@types/passport-github": "^1.1.12",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/pg": "^8.10.7",
    "@types/socket.io-client": "^3.0.0",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^9.0.0",
    "@types/validator": "^13.15.2",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "artillery": "^2.0.23",
    "eslint": "^8.50.0",
    "jest": "^30.0.5",
    "prettier": "^3.0.3",
    "socket.io-client": "^4.8.1",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
