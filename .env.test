# Test Environment Configuration
NODE_ENV=test
PORT=8088

# Database
DATABASE_URL=postgres://test:test@localhost:5432/coderunner_test
TEST_DATABASE_URL=postgres://test:test@localhost:5432/coderunner_test

# Authentication
JWT_SECRET=test-jwt-secret-key-for-testing-only-not-production
JWT_EXPIRES_IN=1h

# Encryption
ENCRYPTION_KEY=test-encryption-key-32-characters-long

# External Services (disabled for tests)
AGENTSPHERE_DOMAIN=agentsphere.run
AGENTSPHERE_API_KEY=test-agentsphere-key
DISABLE_EXTERNAL_CALLS=true

# Logging
LOG_LEVEL=error
TEST_DEBUG=false

# Performance
RATE_LIMIT_ENABLED=false

# WebSocket
WS_ENABLED=true